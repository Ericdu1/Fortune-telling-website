{
  "framework": "create-react-app",
  "buildCommand": "CI=false npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/[^.]+", "dest": "/index.html" }
  ],
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false",
    "NODE_ENV": "production"
  }
} 